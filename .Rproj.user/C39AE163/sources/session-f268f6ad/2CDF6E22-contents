library(shiny)
library(shinythemes)

# 定义 UI
ui <- navbarPage(
  theme = shinytheme("spacelab"),
  title = "Ait",
  
  # 首页标签
  homepage_ui("home_id"),
  
  # 数据标签
  first_tab_ui("first_tab_id"),
  
  # 分析标签
  second_tab_ui("second_tab_id")
)

# 定义服务器逻辑
server <- function(input, output, session) {}

# 运行应用
shinyApp(ui = ui, server = server)
